{
  "name": "oas-wizard",
  "version": "2.0.6",
  "description": "OpenAPI Spec generator using a (yaml) resource sample as starting point",
  "main": "index.js",
  "scripts": {
    "start": "node index.js ",
    "lint": "eslint index.js commands.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "test": "mocha ./tests/test.js",
    "coverage": "nyc mocha ./tests/test.js"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isa-group/oas-wizard.git"
  },
  "dependencies": {
    "commander": "^4.1.1",
    "js-yaml": "^3.13.1",
    "json-schema-generator": "^2.0.6",
    "mustache": "^4.0.0",
    "snyk": "^1.860.0"
  },
  "devDependencies": {
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0"
  },
  "snyk": true,
  "nyc": {
    "exclude": [
      "node_modules",
      "tests/**.js"
    ],
    "reporter": [
      "json",
      "html",
      "text",
      "lcovonly"
    ]
  },
  "bin": {
    "oas-wizard": "./index.js"
  },
  "preferGlobal": true
}
